{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://43.205.60.23/v1/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "upgrade-insecure-requests"
        }
      ]
    }
  ]
}
